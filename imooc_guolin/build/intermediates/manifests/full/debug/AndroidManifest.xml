<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guolin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/guolin"
        android:label="@string/app_name"
        android:theme="@style/MMTheme.Basic" >
        <activity
            android:name="com.guolin.MainActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustResize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.guolin.ContactsActivity" >
        </activity>
        <activity android:name="com.guolin.PullToFreshActivity" >
        </activity>
        <activity android:name="com.guolin.RenrenSlideMenuActivity" >
        </activity>
        <activity android:name="com.guolin.RotatePicBrowserActivity" >
        </activity>
        <activity
            android:name="com.guolin.WeChatActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity android:name="com.guolin.SlidingLayoutActivity" >
        </activity>
        <activity android:name="com.guolin.SlidingViewSwitcherActivity" >
        </activity>
        <activity android:name="com.guolin.ThreeDSlidingLayoutActivity" >
        </activity>
        <activity android:name="com.guolin.PhotosWallActivity" >
        </activity>
        <activity android:name="com.guolin.PhotoWallFallsActivity" >
        </activity>
        <activity android:name="com.guolin.ImageDetailsActivity" >
        </activity>
        <activity android:name="com.guolin.FragmentActivity" >
        </activity>
    </application>

</manifest>